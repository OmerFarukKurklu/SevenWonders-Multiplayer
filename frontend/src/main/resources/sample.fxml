      <Button fx:id="credits_button" mnemonicParsing="false" onAction="#onPressCreditsButton" text="CREDITS" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="4" GridPane.valignment="CENTER" />
      <Button fx:id="exit_button" mnemonicParsing="false" onAction="#onPressExitButton" text="EXIT" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="4" GridPane.valignment="CENTER" />
   </children>
