   <children>
      <Button fx:id="play_game_button" mnemonicParsing="false" onAction="#onPressPlayGameButton" text="PLAY GAME" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER" />
      <Button fx:id="settings_button" mnemonicParsing="false" onAction="#onPressSettingButton" text="SETTINGS" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER" />
