<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" prefHeight="302.0" prefWidth="504.0" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.main_menu_controller">
   <rowConstraints>
      <RowConstraints minHeight="10.0" percentHeight="100.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" percentHeight="100.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" percentHeight="100.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" percentHeight="100.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" percentHeight="100.0" prefHeight="30.0" />
   </rowConstraints>
   <columnConstraints>
      <ColumnConstraints minWidth="10.0" percentWidth="100.0" prefWidth="100.0" />
